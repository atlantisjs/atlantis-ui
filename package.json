{
  "name": "@atlantis/ui",
  "version": "0.1.0",
  "private": true,
  "author": "1ncounter",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/1ncounter/atlantis-ui"
  },
  "main": "dist/atlantis-ui.cjs.js",
  "module": "dist/atlantis-ui.esm.js",
  "jsdelivr": "dist/atlantis-ui.global.js",
  "unpkg": "dist/atlantis-ui.global.js",
  "types": "types/index.d.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "files": [
    "dist/",
    "lib/",
    "es/",
    "types/"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "site:serve": "vue-cli-service serve",
    "site:build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "keywords": [
    "ui",
    "vue",
    "frontend",
    "component",
    "components"
  ],
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "classnames": "^2.2.6"
  },
  "peerDependencies": {
    "vue": "^3.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/classnames": "^2.2.10",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.28.0",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue": "^3.0.2",
    "vuex": "^4.0.0-0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
